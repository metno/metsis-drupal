{% if attributes.class == 'collapsed' %}

<details {{ attributes }}>
  {% if legend %}
    <summary>{{ legend }}</summary>
  {% endif %}
  {% include 'views-view-fields.html.twig' %}
</details>
{% else %}
<details open="">
  {% if legend %}
    <summary>{{ legend }}</summary>
  {% endif %}
  {% include 'views-view-fields.html.twig' %}
</details>
{% endif %}
